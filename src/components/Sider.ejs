<!-- 侧边栏 -->
<aside class="main-sidebar sidebar-dark-primary elevation-4">
  <!-- Brand Logo -->
  <a href="index3.html" class="brand-link">
    <img
      src="img/logo.jpg"
      alt="广告管理LOGO"
      class="brand-image img-circle elevation-3"
      style="opacity: 0.8"
    />
    <span class="brand-text font-weight-light">广告后台管理系统</span>
  </a>

  <!-- Sidebar -->
  <div class="sidebar">
    <!-- Sidebar user panel (optional) -->
    <div class="user-panel mt-3 pb-3 mb-3 d-flex">
      <div class="info">
        <a href="#" class="d-block">欢迎您：xxxxx</a>
      </div>
    </div>

    <!-- Sidebar Menu -->
    <nav class="mt-2">
      <ul
        class="nav nav-pills nav-sidebar flex-column"
        data-widget="treeview"
        role="menu"
        data-accordion="false"
      >
        <%data.routers.filter(item=>item.isNav).forEach(item=>{%>
        <li class="nav-item menu-open">
          <a
            href="javascript:;"
            class="nav-link <%=data.url===item.path?'active':' '%>"
            onclick=" router.go('<%=item.path%>')"
          >
            <i class="nav-icon fas <%=item.icon%>"></i>
            <p><%=item.title%></p>
          </a>
        </li>
        <%})%>
